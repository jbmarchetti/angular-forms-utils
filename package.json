{
  "name": "angular-forms-utils",
  "version": "0.4.2",
  "description": "Angular2 Reactive forms utils",
  "main": "./bundles/angular-forms-utils.umd.js",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "start": "concurrently --raw \"webpack-dev-server --open\" ",
    "startOld": "concurrently --raw \"webpack-dev-server --open\" \"npm run test:watch\"",
    "build:demo": "webpack -p",
    "build:umd": "webpack --config webpack.config.umd.ts",
    "build:ngc": "ngc -p tsconfig-ngc.json",
    "build:dist": "npm run build:umd && npm run build:ngc",
    "build:clean": "del-cli dist",
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "commit": "git-cz",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivateOrInternalSupport",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit --no-ff && npm run build:demo && npm run compodoc && git add . && git commit -m \"chore: build demo and docs\" && git push && git checkout master",
    "copyfiles": "copyfiles package.json LICENSE README.md CHANGELOG.md dist",
    "release:git": "git add . && git commit -m \"chore: bump version number\" && standard-version  && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "commitmsg": "validate-commit-msg",
    "codecov": "cat coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbmarchetti/angular-forms-utils.git"
  },
  "keywords": [
    "angular2",
    "angular"
  ],
  "author": "Jean Baptiste Marchetti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbmarchetti/angular-forms-utils/issues"
  },
  "homepage": "https://github.com/jbmarchetti/angular-forms-utils#readme",
  "devDependencies": {
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/compiler-cli": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@ngui/datetime-picker": "^0.16.2",
    "@ngx-translate/core": "^9.1.1",
    "@types/chai": "^4.0.0",
    "@types/jasmine": "2.8.6",
    "@types/node": "9.4.6",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.28",
    "@types/webpack": "^3.8.7",
    "angular-2-dropdown-multiselect": "^1.4.2",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "^4.0.2",
    "codecov-lite": "^0.1.3",
    "codelyzer": "^4.1.0",
    "commitizen": "^2.9.6",
    "compodoc": "^0.0.41",
    "concurrently": "^3.4.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "^1.0.0",
    "husky": "^0.14.3",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.6.3",
    "karma": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "moment": "^2.18.1",
    "ng2-daterangepicker": "^2.0.10",
    "ngx-treeview": "^2.0.4",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "^0.5.1",
    "rxjs": "^5.4.0",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.10.0",
    "standard-version": "^4.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.7.1",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^3.11.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.12"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "@ngx-translate/core": ">=6.0.0",
    "ngx-treeview": "^2.0.4",
    "moment": ">=2.18.1",
    "ng2-daterangepicker": "^2.0.10",
    "angular-2-dropdown-multiselect": "^1.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
